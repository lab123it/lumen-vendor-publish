{
    "name": "lab123/lumen-vendor-publish",
    "description": "Command for publish vendor configs to Lumen 5.x",
    "homepage": "https://github.com/lab123/lumen-vendor-publish",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Pierre",
            "email": "jean.pierre@lab123.com.br",
            "homepage": "http://lab123.com.br",
            "role": "Developer"
        },  
        {
            "name": "Daniel Reis",
            "email": "daniel.reis@lab123.com.br",
            "homepage": "http://lab123.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^5.7",
        "illuminate/console": "^5.7",
        "illuminate/filesystem":"^5.7",
        "league/flysystem": "1.0.49"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "LumenVendorPublish\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LumenVendorPublish\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
